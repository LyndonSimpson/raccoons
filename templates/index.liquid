<h2>{{ collections['collection_name'].title }}</h2>

{% for product in collections['collection_name'].products %}
    <div>
        <img src="{{ product.featured_image }}">
        <h3>{{ product.title }}</h3>
            <p>{{product.price}} </p>
    {{- product.title | link_to: product.url }}
    </div>
{% endfor %}