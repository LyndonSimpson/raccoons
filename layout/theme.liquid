<html>

    <head>
        <!--built-in data for header - SEO and stuff - metadata - stuff usually in the header-->
        {{content_for_header}}
        {{ 'style.css' | asset_url | stylesheet_tag }} <!-- 'stylesheet_tag makes it a stylesheet link for the head - global CSS stuff goes here' -->
    </head>

    <body>
        <!--built-in data for layout - SEO and stuff.. -->
        {{content_for_layout}}

        <!--header-->
        {% render 'header' %}


        <!--main content of the page - have it be dynamic -->


        <!--footer-->
        {% render 'footer' %}
    </body>

</html>